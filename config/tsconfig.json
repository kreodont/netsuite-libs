{
  "compilerOptions": {
    "target": "es5",
    "module": "AMD",
    "moduleResolution": "node",
    "strict": false,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "outDir": "./src/FileCabinet/SuiteScripts",
    "baseUrl": ".",
    "rootDir": "../",
    "paths": {
      "N/*": [
        "./node_modules/@hitc/netsuite-types/N/*", "../@hitc/netsuite-types/N/*"
      ],
      "N": [
        "./node_modules/@hitc/netsuite-types/N", "../@hitc/netsuite-types/N"
      ],
    }
  },
  "exclude": ["__tests__", "Gulpfile.ts", "deploy.ts"],
  "include": ["*.ts", "netsuite-libs/*.ts"]
}